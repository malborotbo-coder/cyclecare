{
  "version": 2,
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "dist/public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/api/(.*)$",
      "dest": "server/index.js"
    },
    {
      "src": "^/api/auth/google/callback$",
      "dest": "server/index.js"
    },
    {
      "src": "^/(.*)$",
      "dest": "dist/public/index.html"
    }
  ]
}
